{
	"manifest_version": 2,
	"name": "Kahoot.charleyw.com Client",
	"version": "1.0.0",
	"content_scripts": [
		{
			"matches": ["https://kahoot.it/"],
			"js": ["assets/script.js"],
			"css": ["assets/style.css"]
		}
	],
	"icons": {
		"16": "assets/images/kahoot-icon16.png",
		"32": "assets/images/kahoot-icon32.png",
		"48": "assets/images/kahoot-icon48.png",
		"128": "assets/images/kahoot-icon128.png"
	},
	"browser_action": {
		"name": "Kahoot.charleyw.com Client",
		"default_icon": "assets/images/kahoot-icon32.png",
		"default_popup": "assets/popup.html"
	},
	"permissions": ["storage"]
}
